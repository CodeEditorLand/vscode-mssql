					"default": "AuthCodeGrant",
					"description": "%mssql.chooseAuthMethod%",
					"enum": [
						"AuthCodeGrant",
						"DeviceCode"
					],
					"enumDescriptions": [
						"%mssql.authCodeGrant.description%",
						"%mssql.deviceCode.description%"
					],
					"scope": "application",
					"type": "string"
				},
				"mssql.connections": {
					"description": "%mssql.connections%",
					"items": {
						"properties": {
							"applicationIntent": {
								"default": "ReadWrite",
